{
  "name": "env-switcher",
  "displayName": "Env switcher",
  "description": "Quickly switch between production and staging environments in your .env files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.2"
  },
  "author": {
    "name": "Stian Martinsen"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "envSwitcher.changeEnvironment",
        "title": "Change Environment",
        "category": "Env Switcher"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "envSwitcher.changeEnvironment",
          "when": "resourceFilename =~ /\\.env/"
        }
      ],
      "editor/context": [
        {
          "command": "envSwitcher.changeEnvironment",
          "when": "resourceFilename =~ /\\.env/",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "envSwitcher.changeEnvironment",
        "key": "alt+ctrl+e",
        "mac": "alt+cmd+e",
        "when": "resourceFilename =~ /\\.env/"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  },
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a",
  "vsce": {
    "dependencies": false
  }
}